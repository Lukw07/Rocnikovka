{
  "name": "edurpg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo --port 3000",
    "dev:clean": "rm -rf .next && next dev --port 3000",
    "dev:port": "next dev --turbo --port 3001",
    "dev:config": "NODE_ENV=development next dev --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "setup:dev": "copy dev.env .env.local && npm run prisma:generate",
    "clean": "rm -rf .next node_modules/.cache"
  },
  "dependencies": {
    "@prisma/client": "^7.0.0",
